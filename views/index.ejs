            blabla2
          </div><!-- /.well -->
          blabla3
        </div><!-- /.tabpanel -->

        <div class="row">
       
          <div class="col-md-12" style="display:none">

            <div class="panel panel-default" style="display:none">
              <div class="panel-heading" style="display:none">
                <h3>Metadata</h3>
                <span class="pull-right clickable"><i class="caret-up" style="  margin-top: -1.3em;"></i></span>
              </div>
              <div class="panel-body">

                <!-- Table -->
                <div class="table-scroll" style="display:none">
                  <table id="metadataTable">
                    <thead>
                      <tr class="table-header-row">
                        <th>WORD</th>
                        <th>START TIME(sec)</th>
                        <th>END TIME(sec)</th>
                        <th>CONFIDENCE</th>
                      </tr>
                    </thead>
                    <tbody id="metadataTableBody">
                    </tbody>
                  </table>
                </div>

                <!-- END Table -->
                <div class="alt-hypothesis" style="display:none">
                  <h5>Alternate Hypotheses</h5><hr>
                  <div class="hypotheses">
                    <ol></ol>
                  </div>
                </div>
              </div>
            </div>

            <!-- Spacer-->
            <!-- TODO: Move this to CSS-->
            <div class="row" style="display:none">
              <div class="col-lg-12 col-md-12 col-sm-12">
                <br><br>
              </div>
            </div>


          </div><!-- /.col -->
        </div><!-- /.row -->

      </div><!-- /.container -->

      <!-- for file upload dialog in toolbar -->
      <input style="display:none" type="file" id="fileUploadDialog">
	  <div class='full' >
